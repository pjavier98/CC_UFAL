#include <stdio.h>
#include <stdlib.h>

void troca(int *a, int *b)
{
	int temp;

	if (*a > *b)
	{
		temp = *a;
		*a = *b;
		*b = temp;
	}
}

void comparar(int num[], int i, int j, int quant)
{
	if (quant == i)
	{
	    return;
	}

	if (j < quant)
	{
		if (num[i] > num[j])
		{
			troca(&num[i], &num[j]);
		}

		comparar(num, i, j + 1, quant);
		
		return;
	}

	
	comparar(num, i + 1, i + 1, quant);

}

void ler(int numeros[], int i, int *quant)
{
	scanf("%d", &numeros[i]);
	
	if (numeros[i] == 99)
	{
		return;
	}

	*quant += 1;

	ler(numeros, i + 1, quant);
}

int main()
{
	int *numeros = malloc(1000 * sizeof(int));
    
    int quant = 0;
    
	ler(numeros, 0, &quant);

	comparar(numeros, 0, 1, quant);
	
	printf("%d\n", numeros[quant - 2]);
        
	return 0;
}